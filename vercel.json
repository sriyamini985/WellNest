{
  "version": 2,
  "builds": [
    {
      "src": "WellNest/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/WellNest/index.html"
    }
  ],
  "buildCommand": "cd WellNest && npm install && npm run build"
}
